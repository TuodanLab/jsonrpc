{
  "name": "@tuodanlab/jsonrpc",
  "version": "7.6.0",
  "description": "JSON RPC module for NestJS framework",
  "author": "xavier",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "dist/**/*",
    "LICENSE"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.9",
    "@types/node": "12.12.54",
    "@types/supertest": "2.0.10",
    "jest": "29.0.0",
    "prettier": "^3.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.8.0",
    "supertest": "4.0.2",
    "ts-jest": "29.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TuodanLab/jsonrpc"
  },
  "bugs": {
    "email": "tuodanlab@gmail.com",
    "url": "https://github.com/TuodanLab/jsonrpc/issues"
  }
}
