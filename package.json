{
  "name": "@tuodan/jsonrpc",
  "version": "1.0.1",
  "description": "JSON RPC module for NestJS framework",
  "author": "xavier",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "**/*",
    "LICENSE"
  ],
  "scripts": {
    "release": "nest build && cp package.json README.md LICENSE ./dist && cd ./dist && npm publish",
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.9",
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/testing": "^9.3.9",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.3",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.9",
    "@types/node": "^18.11.18",
    "@types/supertest": "2.0.10",
    "jest": "29.0.0",
    "prettier": "^3.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.8.0",
    "supertest": "4.0.2",
    "ts-jest": "29.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TuodanLab/jsonrpc"
  },
  "bugs": {
    "email": "tuodanlab@gmail.com",
    "url": "https://github.com/TuodanLab/jsonrpc/issues"
  },
  "dependencies": {
    "@tuodanlab/logger": "^1.0.1",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
